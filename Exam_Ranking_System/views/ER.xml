<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_examresult_tree" model="ir.ui.view">
            <field name="name">exam.result.tree</field>
            <field name="model">exam.result</field>
            <field name="arch" type="xml">
                <tree string="examresult">
                    <!--<field name="studentid"/>
                    <field name="exam_id"/>-->
                    <field name="marks_obtained"/>
                </tree>
            </field>
        </record>

        <record id="view_examresult_form" model="ir.ui.view">
            <field name="name">exam.result.form</field>
            <field name="model">exam.result</field>
            <field name="arch" type="xml">
                <form string="Examresult">
                    <group>
                        <!--<group>
                            <field name="student_id"/>
                            <field name="exam_id"/>
                        </group>-->
                        
                        <group>
                            <field name="marks_obtained"/>
                        </group>
                    </group>
                   
                </form>
            </field>
        </record>

        <record id="view_examresult_kanban" model="ir.ui.view">
            <field name="name">exam.result.kanban</field>
            <field name="model">exam.result</field>
            <field name="arch" type="xml">
                <kanban>
                    <!--<field name="student_id"/>
                    <field name="exam_id"/>-->
                    <field name="marks_obtained"/>
                </kanban>
            </field>
        </record>

        <record id="action_examresult_form" model="ir.actions.act_window">
            <field name="name">Examresult</field>
            <field name="res_model">exam.result</field>
            <field name="view_mode">tree,form,kanban</field>
            
            
        </record>

    </data>
</odoo>
